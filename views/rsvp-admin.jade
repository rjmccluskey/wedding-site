- var yeses;
- var nos;
- var songs;

extends page

block pageContent
    .pure-g
        .pure-u-1-3
            h4 Yeses:
            ul
                each yes in yeses
                    li= yes
        .pure-u-1-3
            h4 Nos:
            ul
                each no in nos
                    li= no
        .pure-u-1-3
            h4 Songs:
            ul
                each song in songs
                    li= song